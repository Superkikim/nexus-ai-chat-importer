{
    "name": "nexus-ai-chat-importer",
    "version": "1.0.3",
    "description": "This plugin allows to import ChatGPT conversation from the export files downloaded from your account",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc --noEmit --skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "lint": "eslint . --ext .ts,.js",
        "lint:fix": "eslint . --ext .ts,.js --fix",
        "format": "prettier --write \"**/*.{ts,js,json,md}\"",
        "ios-build": "tsc --noEmit --skipLibCheck && node esbuild.config.mjs ios"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.17.3",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "4.7.4",
        "glob": "^9.3.5",
        "rimraf": "^4.1.2",
        "@eslint/eslintrc": "^1.0.0",
        "eslint": "^8.0.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.3.2",
        "copyfiles": "^2.3.0",
        "fs-extra": "^11.2.0"
    },
    "dependencies": {
        "jszip": "^3.7.1"
    }
}
